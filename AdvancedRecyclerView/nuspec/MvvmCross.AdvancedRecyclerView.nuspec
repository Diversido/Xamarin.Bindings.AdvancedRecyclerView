<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>MvvmCross.AdvancedRecyclerView</id>
    <version>1.12.0</version>
    <title>MvvmCross.Bindings.AdvancedRecyclerView</title>
    <authors>Przemyslaw Raciborski [thefex]</authors>
    <projectUrl>https://github.com/thefex/Xamarin.Bindings.AdvancedRecyclerView</projectUrl>
    <licenseUrl>https://github.com/thefex/Xamarin.Bindings.AdvancedRecyclerView/blob/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>
        Mvx Wrapper for AdvancedRecyclerView library.
        Currently supports:
        - grouping,
        - swipe (without grouping),
        - header/footer
    </description>
    <tags>xamarin android mvvmcross</tags>
    <dependencies>
      <group targetFramework="monoandroid">
          <dependency id="MvvmCross.Binding" version="6.1.2"/>
          <dependency id="MvvmCross.Droid.Support.V7.RecyclerView" version="6.1.2" />
          <dependency id="Xamarin.Android.Support.Animated.Vector.Drawable" version="27.0.2" />
          <dependency id="Xamarin.Android.Support.Annotations" version="27.0.2" />
          <dependency id="Xamarin.Android.Support.Compat" version="27.0.2"  />
          <dependency id="Xamarin.Android.Support.Core.UI" version="27.0.2"  />
          <dependency id="Xamarin.Android.Support.Core.Utils" version="27.0.2" />
          <dependency id="Xamarin.Android.Support.Fragment" version="27.0.2"  />
          <dependency id="Xamarin.Android.Support.Media.Compat" version="27.0.2" />
          <dependency id="Xamarin.Android.Support.v7.AppCompat" version="27.0.2" />
          <dependency id="Xamarin.Android.Support.v7.RecyclerView" version="27.0.2" />
          <dependency id="Xamarin.Android.Support.Vector.Drawable" version="27.0.2" />
          <dependency id="Xamarin.Build.Download" version="0.4.7" />
          <dependency id="Xamarin.Bindings.AdvancedRecyclerView" version="1.11.0.0" />
        </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\MvvmCross.AdvancedRecyclerView\bin\Release\MvvmCross.AdvancedRecyclerView.*" target="lib\MonoAndroid" />
  </files>
</package>













